#include "IO_control.h"

void dianfengshan_IO(uchar a){

	infoMal_teacher.dianfengshan |= (1 << (a-1));
}

void weibolu_IO(uchar a){

	infoMal_teacher.weibolu |= (1 << (a-4));
}

void dianfanbao_IO(uchar a){

	infoMal_teacher.dianfanbao |= (1 << (a-6));
}

void xiyiji_IO(uchar a){

	infoMal_teacher.xiyiji |= (1 << (a-9));
}

void xiaodugui_IO(uchar a){

	infoMal_teacher.xiaodugui |= (1 << (a-12));
}